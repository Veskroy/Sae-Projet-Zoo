{% extends 'base.html.twig' %}

{% block title %}Ticket{% endblock %}
{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('css/base.css')}}" type="text/css">
{% endblock %}

{% block body %}

    <h1>Liste de Ticket Achete</h1>

    {% for ticket in tickets %}

    <li> <a href='{{ path('app_ticket_id', {"id": ticket.id } ) }}' > Ticket Achete Numero {{ ticket.id}} </a></li>

    {% endfor %}

    <div>{{ form(form) }}</div>
{% endblock %}
